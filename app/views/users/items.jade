#items
  h3 Items
    each item in user.items
      case item.type
        when 'autogrow'
          include ../items/autogrow
            img(src='/img/autogrow.png')
        when 'autoseed'
          include ../items/autoseed
            img(src='/img/autoseed.png')
        when 'autoroot'
          include ../items/autoroot

